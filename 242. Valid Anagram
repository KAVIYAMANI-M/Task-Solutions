class Solution {
    public boolean isAnagram(String s, String t) {
        if(t.length() > s.length() || t.length() < s.length())
        {
            return false;
        }
        int fre[] = new int[128];
        for(int i=0;i<s.length();i++)
        {
            fre[(int)(s.charAt(i))]++;
            fre[(int)(t.charAt(i))]--;
        }
        for(int i=0;i<fre.length;i++)
        {
            if(fre[i] != 0)
            {
                return false;
            }
        }
        return true;
    }
}
